   "source": [
    "a = 1   #fibo(0) \n",
    "b = 1   #fibo(1)\n",
    "c = a + b   \n",
    "\n",
    "sum = 0\n",
    "\n",
    "while (c <= 4000000):\n",
    "    if c % 2 == 0:\n",
    "        sum += c\n",
    "    a = b\n",
    "    b = c\n",
    "    c = a + b\n",
    "\n",
    "print(sum)\n"
   ]
  }
